{
  "version": "1.0.0-*",

  "dependencies": {
    "NETStandard.Library": "1.6.0",
    "NUnit": "3.4.1",
    "dotnet-test-nunit": "3.4.0-beta-2",
    "Itinero.VectorTiles": "1.0.0-*",
    "Itinero.VectorTiles.Mapbox": "1.0.0-*"
  },

  "testRunner": "nunit",


  "frameworks": {
    "net4.5.1": {
      "frameworkAssemblies": {
        "System.Xml": ""
      },
      "buildOptions": {
